{
    "input_transforms": [
        {
            "attr_name": "x",
            "type": "vector",
            "fields": [
                "startX",
                "endX",
                "startY"
            ]
        }
    ],
    "backbone": [
        {
            "type": "gcn",
            "gcn_layers": 4,
            "gcn_repetitions": 1,
            "hidden_dim": 128,
            "layer_type": "GatedGraphConv",
            "activation": "None",
            "input_dims": [3,0,0]
        }
    ],
    "heads": [
        {
            "type": "cls",
            "field": "label",
            "target": "node",
            "classes": [
                "Name",
                "None",
                "Rank",
                "Birth",
                "Nationality",
                "Died",
                "Buried-date",
                "Buried-place",
                "Source-place",
                "Source-book",
                "Grave position"
            ],
            "net_config": {
                "type": "mlp"
            }
        },
        {
            "type": "regr",
            "field": "endY",
            "target": "node",
            "net_config": {
                "type": "mlp"
            }
        }
    ]
}