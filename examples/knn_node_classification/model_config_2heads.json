{
    "input": {
        "type": "vector",
        "fields": [ "startX", "endX", "startY" ]
    },
    "backbone": {
        "type": "gcn",
        "gcn_layers": 4,
        "gcn_repetitions": 1,
        "hidden_dim": 128,
        "layer_type": "GatedGraphConv",
        "activation": "None",
        "input_dim": 3
    },
    "heads": [
        {
            "type": "node_cls",
            "field": "label",
            "classes": [
                "Name",
                "None",
                "Rank",
                "Birth",
                "Nationality",
                "Died",
                "Buried-date",
                "Buried-place",
                "Source-place",
                "Source-book",
                "Grave position"
            ],
            "net_config": {
                "type": "mlp"
            }
        },
        {
            "type": "node_regr",
            "field": "endY",
            "net_config": {
                "type": "mlp"
            }
        }
    ]
}